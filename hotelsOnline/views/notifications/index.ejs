<%- include("../partials/header") %>

<h1>Notifications</h1>

<ul>
	<% allNotifications.forEach(function(notification) { %>
	<li>
		<a href="/hotels/<%= notification.hotelId %>">
			<%= notification.username %> added a new hotel %>
		</a>
	</li>
	<% }); %>
</ul>

